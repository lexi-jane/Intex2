@{
}

@model IEnumerable<string>


    <div class="d-grid gap-2">
        <a asp-controller="Home" asp-action="CrashDetailsList" asp-route-countySelect="" class="btn @(ViewBag.SelectedCounty is null ? "btn-primary" : "btn-outline-secondary")">All Counties</a>
        <br />
        @foreach (string c in Model)
        {
            <a asp-controller="Home" asp-action="CrashDetailsList" asp-route-countySelect="@c" 
               class="btn @(ViewBag.SelectedCounty == c ? "btn-primary" : "btn-outline-secondary")">@c</a>
            <br />
        }
    </div>
